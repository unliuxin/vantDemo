.van-col{position: relative;}
.van-col::before {
  content: '';
  width: 10rpx;
  height: 20rpx;
  border-radius: 0 15rpx 15rpx 0;
  background-color: #e66700;
  position: absolute;
  left: 0;
  top: 40%;
}
.van-col::after {
  content: '';
  width: 10rpx;
  height: 20rpx;
  border-radius: 15rpx 0 0 15rpx;
  background-color: #e66700;
  position: absolute;
  right: 0;
  top: 40%;
}

.suo_dec{
  position: relative;
}
.suo_dec::before {
  content: '';
  width: 10rpx;
  height: 20rpx;
  border-radius: 0 15rpx 15rpx 0;
  background-color: #fff;
  position: absolute;
  left: 0;
  top: 40%;
}
.suo_dec::after {
  content: '';
  width: 10rpx;
  height: 20rpx;
  border-radius: 15rpx 0 0 15rpx;
  background-color: #fff;
  position: absolute;
  right: 0;
  top: 40%;
}